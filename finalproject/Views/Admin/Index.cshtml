@model finalproject.Models.ApplicationViewModel

@{
    ViewData["Title"] = "Index";
}




            <div class="m-content">

                <div class="m-portlet m-portlet--mobile">
                    @Html.Partial("dataheader")
                    <div class="m-portlet__body">
                        <!--begin: Search Form -->
                        @Html.Partial("_searchsort")
                        <!--end: Search Form -->
                        <!--begin: Datatable -->
                        <table class="m-datatable" id="html_table" width="100%">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Admin.Name)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Admin.CNIC)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Admin.Age)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Admin.Email)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Admin.Password)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Admin.userimages)
                                    </th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @foreach (var item in Model.Admins)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.Name)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.CNIC)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Age)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Email)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Password)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.userimages)
                        </td>
                        <td>
                            <div class="btn-group m-btn-group m-btn-group--pill m-btn-group--air">
                                <a asp-action="Edit" asp-route-id="@item.ID" class="m-btn btn btn-brand">Edit</a>
                                <a asp-action="Delete" asp-route-id="@item.ID" class="btn btn-danger Delete">Delete</a>
                            </div>
                        </td>
                    </tr>
}
                            </tbody>
                        </table>
                        <!--end: Datatable -->
                    </div>
                </div>
            </div>



