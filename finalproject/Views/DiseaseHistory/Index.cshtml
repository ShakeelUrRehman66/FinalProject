@model IEnumerable<finalproject.Models.DiseaseHistory>

@{
    ViewData["Title"] = "Index";
}




            <div class="m-content">

                <div class="m-portlet m-portlet--mobile">
                    @Html.Partial("dataheader")
                    <div class="m-portlet__body">
                        <!--begin: Search Form -->
                        @Html.Partial("_searchsort")
                        <!--end: Search Form -->
                        <!--begin: Datatable -->
                        <table class="m-datatable" id="html_table" width="100%">
                            <thead>
                                <tr>
                                    <th>
                                        @Html.DisplayNameFor(model => model.DiseaseName)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.Type)
                                    </th>
                                    <th>
                                        @Html.DisplayNameFor(model => model.duration)
                                    </th>

                                    <th></th>
                                </tr>
                            </thead>

                            <tbody>
                                @foreach (var item in Model)
                {
                    <tr>
                        <td>
                            @Html.DisplayFor(modelItem => item.DiseaseName)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.Type)
                        </td>
                        <td>
                            @Html.DisplayFor(modelItem => item.duration)
                        </td>

                        <td>
                            <a asp-action="Edit" asp-route-id="@item.DisHisID">Edit</a> |
                            <a asp-action="Details" asp-route-id="@item.DisHisID">Details</a> |
                            <a asp-action="Delete" asp-route-id="@item.DisHisID">Delete</a>
                        </td>
                    </tr>
}
                            </tbody>
                        </table>
                        <!--end: Datatable -->
                    </div>
                </div>
            </div>




